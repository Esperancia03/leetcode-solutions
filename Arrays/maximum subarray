class Solution {
public:
    int maxSubArray(vector<int>& nums) {
     
      int current_best_sum = nums[0];
      int max_sum = nums[0];     
      for(int i=1; i<nums.size(); i++)
     {    
        current_best_sum = max(nums[i], nums[i] + current_best_sum);
     max_sum = max(max_sum, current_best_sum);       

    }

    return max_sum;
    }
};
